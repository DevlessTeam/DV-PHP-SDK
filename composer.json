{
    "name": "devless/php-sdk",
    "description": "official Devless SDK",
    "keywords": [
        "Devless",
        "php",
        "SDK",
    ],
    "homepage": "Devless.io",
    "license": "APACHE 2.0",
    "authors": [
        {
            "name": "Eddymens",
            "email": "edmond@devless.io",
            "homepage": "Devless.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.*"
    },
    "autoload": {
        "psr-4": {
            "Devless\\sdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devless\\sdk\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
